# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/mental-health-tracker
# Or for MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/mental-health-tracker

# n8n Webhook Configuration
N8N_WEBHOOK_URL=http://localhost:5678/webhook/mood-analysis
N8N_WEBHOOK_TOKEN=your_optional_webhook_token

# OpenAI Configuration (if using OpenAI directly instead of n8n)
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-3.5-turbo

# Optional: Rate limiting and security
MAX_MOOD_LENGTH=2000
WEBHOOK_TIMEOUT_MS=30000 